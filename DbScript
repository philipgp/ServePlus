create table USER(
ID int  auto_INCREMENT PRIMARY KEY,
FIRST_NAME VARCHAR2(30),
MIDDLE_NAME VARCHAR2(30),
LAST_NAME VARCHAR2(30),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);



create table customer(
 ID INT auto_INCREMENT PRIMARY KEY,
  USER_ID INT REFERENCES USER(ID),
  CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);



CREATE TABLE COMPANY(
ID INT PRIMARY KEY,
NAME VARCHAR2(50),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);
create table ASSIGNER( 
ID INT PRIMARY KEY , 
USER_ID INT REFERENCES USER(ID),
COMPANY_ID INT REFERENCES COMPANY(ID),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);
create table CATEGORY( 
ID INT PRIMARY KEY, 
NAME VARCHAR2(50),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);
create table SERVICE(
 ID INT PRIMARY KEY,
  NAME VARCHAR2(50),
   CATEGORY_ID INT REFERENCES CATEGORY(ID),
   CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
   );
create table WORKER_SERVICE( 
WORKER_ID INT REFERENCES WORKER(ID),
 SERVICE_ID INT REFERENCES SERVICE(ID),
 ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
 PRIMARY KEY(WORKER_ID ,SERVICE_ID),
 CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table LOCATION(
ID int   AUTO_INCREMENT PRIMARY KEY, 
LATITIDE VARCHAR2(20),
LONGITUDE  VARCHAR2(20),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
); 



create table ADDRESS(
ID int  AUTO_INCREMENT PRIMARY KEY, 
FIRST_LINE VARCHAR2(20),
SECOND_LINE VARCHAR2(20),
LOCATION_ID INT REFERENCES LOCATION(ID),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
); 


create table SR_ACTION_HISTORY(
ID int   PRIMARY KEY, 
SERVICE_REQUEST_ID INT REFERENCES SERVICE_REQUEST(ID),
ACTION VARCHAR2(30),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);

create table WORKER( 
ID INT PRIMARY KEY , 
USER_ID INT REFERENCES USER(ID),
COMPANY_ID INT REFERENCES COMPANY(ID),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table USER_ADDRESS( 
id number AUTO_INCREMENT PRIMARY KEY,
USER_ID INT REFERENCES USER(ID), 
ADDRESS_ID INT REFERENCES ADDRESS(ID),
ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table COMPANY_ADDRESS( 
COMPANY_ID INT REFERENCES USER(ID), 
ADDRESS_ID INT REFERENCES ADDRESS(ID),
ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
PRIMARY KEY(COMPANY_ID ,ADDRESS_ID ),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);

CREATE TABLE SERVICE_REQUEST( 
ID INT AUTO_INCREMENT  PRIMARY KEY,
REQUEST VARCHAR2(200),
SERVICE_ID INT REFERENCES SERVICE(ID),
CUSTOMER_ID INT REFERENCES CUSTOMER(ID),
ASSIGNER_ID INT REFERENCES ASSIGNER(ID),
WORKER_ID INT REFERENCES WORKER(ID),
PAYMENT_ID NUMBER REFERENCES PAYMENT(ID),
ADDRESS_ID INT REFERENCES ADDRESS(ID),
PREFERRED_TIME VARCHAR2(15),
PREFERRED_DAYS VARCHAR2(25),
STATUS VARCHAR2(20),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

CREATE TABLE SR_ATTACHMENTS ( ID INT PRIMARY KEY,
SERVICE_REQUEST_ID INT REFERENCES SERVICE_REQUEST(ID),
ATT_FILE_NAME VARCHAR2(30),
FILE_TYPE VARCHAR2(30),
BINARY BLOB,
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE)

create table CONTACT_DETAIL(
ID int  PRIMARY KEY, 
TYPE  VARCHAR2(30),
VALUE  VARCHAR2(30),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);

create table SR_CONTACT(
ID NUMBER AUTO_INCREMENT PRIMARY KEY,
SR_ID INT REFERENCES SERVICE_REQUEST(ID), 
CONTACT_DETAIL_ID INT REFERENCES CONTACT_DETAIL(ID),

CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);

create table USER_CONTACT(
ID NUmber AUTO_INCREMENT PRIMARY KEY,
USER_ID INT REFERENCES USER(ID), 
CONTACT_DETAIL_ID INT REFERENCES CONTACT_DETAIL(ID),
ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE
);




CREATE TABLE PAYMENT( ID int  PRIMARY KEY, 
TYPE  VARCHAR2(30),
AMOUNT number(10,3),
STATUS VARCHAR2(30),
HANDLER_TYPE VARCHAR2(20),
HANDLER_ID INT ,
CURRENCY VARCHAR2(30),	
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);



create table COMPANY_SERVICE( 
ID NUMBER AUTO_INCREMENT PRIMARY KEY,
COMPANY_ID NUMBER REFERENCES WORKER(ID),
 SERVICE_ID NUMBER REFERENCES SERVICE(ID),
 ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
 CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table CS_ASSIGNER( 
CS_ID INT REFERENCES COMPANY_SERVICE(ID),
 ASSIGNER_ID INT REFERENCES ASSIGNER(ID),
 ACTIVE BOOLEAN,
DEFAULT BOOLEAN,
 PRIMARY KEY(CS_ID ,ASSIGNER_ID ),
 CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table CS_RATING_QUESTIONS(
ID NUMBER AUTO_INCREMENT PRIMARY KEY,
CS_ID INT REFERENCES COMPANY_SERVICE(ID),
QUESTION VARCHAR2(30),
ORDER_N NUMBER(3) ,
QUESTION_TYPE VARCHAR2(10),
 CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table SERVICE_RATING_QUESTIONS(
ID NUMBER AUTO_INCREMENT PRIMARY KEY,
SERVICE_ID INT REFERENCES SERVICE(ID),
QUESTION VARCHAR2(30),
ORDER_N NUMBER(3) ,
QUESTION_TYPE VARCHAR2(10),
 CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

CREATE TABLE SR_RATING( ID int  auto_INCREMENT PRIMARY KEY, 
SERVICE_REQUEST_ID   NUMBER REFERENCES SERVICE_REQUEST(ID),
QUESTION_ID NUMBER,
QUESTION_SOURCE VARCHAR2(15),
ANSWER  VARCHAR2(30),
CREATED_DATE DATE,
LAST_MODIFIED_DATE DATE);

create table HOST_PROP(ID INT AUTo_increment PRIMARY KEY, 
KEY VARchar2(30),VALUE VARCHAR2(50))